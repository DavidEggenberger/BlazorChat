<div class="card contact">
    <div class="card-body">
        <h2>@Node.UserDto.Name</h2>
        <img src="@Node.UserDto.Image" />
    </div>
    <div class="portContainer">
        @foreach (var port in Node.Ports)
        {
            <PortRenderer Port="port" />
        }
    </div>
</div>

@code{
    [Parameter]
    public ContactNode Node { get; set; }
}
